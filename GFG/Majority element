#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t){
        int n,maxcount = 0,index = -1;
        cin>>n;
        int a[n];
        for(int i = 0; i < n; i++)
        {
            cin>>a[i];
        }
        for(int i = 0; i < n; i++)
        {   
            int count = 0;
            for(int j = 0; j < n; j++)
            {
                if(a[i] == a[j])
                {
                    count++;
                }
            }

            if(count > maxcount)
            {
                maxcount = count;
                index = i;
            }
            
            
        }
        if(maxcount > n/2)
        {
            cout<<a[index]<<endl;
        }
        else
        {
            cout<<"-1"<<endl;
        }
        
        t--;
    }
    return 0;
}
