#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,no_of_elements;
        cin>>n>>no_of_elements;
        int a[n];
        for(int i = 0; i < n; i++)
        {
            cin>>a[i];
        }
        sort(a,a+n);
        for(int i = n-1; i > n-1-no_of_elements; i--)
        {
            cout<<a[i]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
