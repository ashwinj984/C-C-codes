#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        vector<int> vr;
        while(n!=0)
        {
            
            int m = n % 2;
            n = n / 2;
            vr.push_back(m);
        }
        int k = vr.size();
        for(int i = k-1 ;i >= 0; i--)
        {
            cout<<vr[i];
        }
        cout<<endl;
    }
    return 0;
}
