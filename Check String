#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        int count = 0;
        int n = s.size();
        for(int i = 0; i < n; i++)
        {
            if(s[0] != s[i])
            {
                cout<<"NO"<<endl;
                break;
            }
            else if(s[0] == s[i])
            count++;

        }
        if(count == n)
        cout<<"YES"<<endl;

    }
    return 0;
}
